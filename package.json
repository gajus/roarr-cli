{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/roarr/**/*"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "bin": {
    "roarr": "dist/src/bin/index.js"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "es6-error": "^4.1.1",
    "liqe": "^1.13.0",
    "prettyjson": "^1.2.1",
    "roarr": "^7.8.0",
    "socket.io-client": "^4.4.0",
    "split2": "^4.1.0",
    "yargs": "^17.3.0"
  },
  "description": "A CLI program for Roarr logger.",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/split2": "^3.2.1",
    "@types/yargs": "^17.0.7",
    "ava": "^3.15.0",
    "coveralls": "^3.1.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.4.0",
    "eslint-config-canonical": "^32.43.3",
    "husky": "^7.0.4",
    "nyc": "^15.1.0",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "log",
    "logger",
    "json"
  ],
  "name": "@roarr/cli",
  "repository": {
    "type": "git",
    "url": "git@github.com:gajus/roarr-cli.git"
  },
  "scripts": {
    "build": "del-cli ./dist && tsc",
    "dev": "tsc --watch",
    "lint": "eslint ./src ./test && tsc --noEmit",
    "test": "NODE_ENV=test ava --serial --verbose"
  },
  "version": "1.0.0"
}
