{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/roarr/**/*"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "files": [
    "./dist"
  ],
  "bin": {
    "roarr": "dist/src/bin/index.js"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "es6-error": "^4.1.1",
    "liqe": "^3.6.0",
    "prettyjson": "^1.2.5",
    "roarr": "^7.14.3",
    "socket.io-client": "^4.6.1",
    "split2": "^4.1.0",
    "yargs": "^17.7.1"
  },
  "description": "A CLI program for Roarr logger.",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@types/split2": "^3.2.1",
    "@types/yargs": "^17.0.7",
    "ava": "^5.2.0",
    "coveralls": "^3.1.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.34.0",
    "eslint-config-canonical": "^40.0.7",
    "husky": "^7.0.4",
    "nyc": "^15.1.0",
    "semantic-release": "^20.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "log",
    "logger",
    "json"
  ],
  "name": "@roarr/cli",
  "repository": {
    "type": "git",
    "url": "git@github.com:gajus/roarr-cli.git"
  },
  "scripts": {
    "build": "del-cli ./dist && tsc",
    "dev": "tsc --watch",
    "lint": "eslint ./src ./test && tsc --noEmit",
    "test": "NODE_ENV=test ava --serial --verbose"
  },
  "version": "1.0.0"
}
